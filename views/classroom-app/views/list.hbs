<h1>Student List</h1>
<p>
  <a href="/edit">Add Student</a>
</p>
<table>
  <tr>
    <th>Name</th>
    <th></th>
  </tr>
  {{#each students}}
  <tr>
    <td>{{ this.name }}</td>
    <td><a href="/edit/{{ this.id }}">edit</a> <a href="/delete/{{ this.id }}">delete</a></td>
  </tr>
  {{/each}}
</table>